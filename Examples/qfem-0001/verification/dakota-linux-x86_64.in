environment 
 tabular_data 
 tabular_data_file = 'dakotaTab.out' 

method,
 sampling
 sample_type = lhs 
 samples = 50 
 seed = 949

variables 
 active uncertain 
  normal_uncertain = 2
    means = 25 500 
    std_deviations = 3 25 
    descriptors = 'P' 'Au' 
  lognormal_uncertain = 2
    means = 205 250 
    std_deviations = 15 10 
    descriptors = 'E' 'Ao' 



interface 
  analysis_driver = 'workflow_driver1'
  fork
   parameters_file = 'paramsDakota.in'
   results_file = 'results.out' 
   aprepro 
   work_directory
     named 'workdir' 
     directory_tag
     directory_save
     copy_files = 'templatedir/*' 
  asynchronous evaluation_concurrency = 12

responses
 response_functions = 2
 response_descriptors = 'Node_3_Disp_2' 'Node_2_Disp_2' 
 no_gradients
 no_hessians

