environment 
 tabular_data 
 tabular_data_file = 'dakotaTab.out' 

method, 
 local_reliability 
 mpp_search no_approx 
 integration second_order 
 
 num_probability_levels = 6  
 probability_levels = 0.98 0.8 0.6 0.4 0.2 0.01 
	

variables 
 active uncertain 
  normal_uncertain = 1
    means = 500 
    std_deviations = 100 
    descriptors = 'Au' 
  lognormal_uncertain = 1
    means = 250 
    std_deviations = 50 
    descriptors = 'Ao' 
  weibull_uncertain = 1
    alphas = 20 
    betas = 210 
    descriptors = 'E' 
  beta_uncertain = 1
    alphas = 2 
    betas = 2 
    lower_bounds = 20 
    upper_bounds = 30 
    descriptors = 'P' 



interface 
  analysis_driver = 'workflow_driver1'
  fork
   parameters_file = 'paramsDakota.in'
   results_file = 'results.out' 
   aprepro 
   work_directory
     named 'workdir' 
     directory_tag
     directory_save
     copy_files = 'templatedir/*' 
  asynchronous evaluation_concurrency = 12

responses
 response_functions = 1
 response_descriptors = 'Node_3_Disp_2' 
 numerical_gradients
 numerical_hessians

